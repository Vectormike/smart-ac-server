{
  "name": "theorem",
  "version": "1.0.0",
  "description": "API",
  "repository": {
    "type": "git",
    "url": ""
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts",
      "src/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "typings"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  },
  "author": "Victor Jonah",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm run watch-ts",
    "build": "tsc -p src",
    "postinstall": "tsc -p src",
    "quick-build": "npm run build",
    "serve": "set NODE_ENV=development && node dist/src/server.js",
    "start-dev": "set NODE_ENV=development && nodemon dist/server.js",
    "lint": "tslint src/**/*.ts -p tsconfig.json",
    "lint:fix": "tslint --fix -c tslint.json 'src/**/*{.ts}",
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "coverage": "set NODE_ENV=test && jest --coverage --runInBand --detectOpenHandles ",
    "watch-ts": "tsc-watch  --outDir ./dist --onSuccess \"npm run serve\" --lib ES2015 --skipLibCheck",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/register": "^7.13.16",
    "@faker-js/faker": "^6.0.0-alpha.6",
    "@hapi/joi": "^17.1.1",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.2",
    "connection-string": "^4.3.4",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "coveralls": "^3.0.2",
    "cross-env": "^7.0.2",
    "dotenv": "^5.0.1",
    "express": "^4.15.3",
    "express-rate-limit": "^5.1.3",
    "ffprobe": "^1.1.2",
    "helmet": "^3.12.0",
    "http-errors": "^1.8.0",
    "http-status": "^1.4.2",
    "joi": "^17.2.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.6",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "mysql2": "^2.2.5",
    "objection": "^2.2.15",
    "randomstring": "^1.2.2",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.4.1",
    "tsyringe": "^4.5.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5",
    "winston-elasticsearch": "^0.15.6",
    "winston-logstash": "^0.4.0",
    "winston3-logstash-transport": "^1.2.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^3.0.5",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.0.36",
    "@types/hapi__joi": "^17.1.6",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.71",
    "@types/moment": "^2.13.0",
    "@types/node": "^13.13.2",
    "@types/randomstring": "^1.1.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "concurrently": "^3.5.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "husky": "^7.0.1",
    "istanbul": "^1.0.0-alpha.2",
    "jest": "^26.6.3",
    "jest-express": "^1.12.0",
    "jest-extended": "^2.0.0",
    "mocha": "^9.2.1",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^8.10.2",
    "tsc-watch": "^4.2.3",
    "typescript": "^4.3.2"
  },
  "license": "MIT"
}
